# .env.example

# The token for your Discord server-bot
SERVER_BOT_TOKEN=

# The token for your Discord client-bot
CLIENT_BOT_TOKEN=

# The ID of the Discord guild (server) where the bots will operate
DISCORD_GUILD_ID=

# The ID of the Discord channel where metrics will be sent
METRICS_CHANNEL_ID=

# The name of the Discord category where chat sessions will be created
CHAT_CATEGORY_NAME="AI Chat Sessions"

# The URL of your PostgreSQL database
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/ai_chat_system